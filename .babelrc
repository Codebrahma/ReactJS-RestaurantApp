{
  "presets": [
    "stage-2",
    "es2015",
    "react",
  ],
  "env": {
    "production": {
      "only": [
        "app"
      ],
      "plugins": [
        "react-require",
        "transform-react-remove-prop-types",
        "transform-react-constant-elements",
        "transform-react-inline-elements",
        "add-module-exports",
        "react-transform"
      ]
    }
  }
}
